### Copy this file to .env and fill values to enable Twilio WhatsApp/SMS notifications
# Twilio credentials
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
# Twilio WhatsApp 'from' number in the format 'whatsapp:+1415xxxxxxx'
TWILIO_WHATSAPP_FROM=

# Optional: Twilio phone number for SMS fallback (in E.164, e.g. +1415xxxxxxx)
TWILIO_PHONE_FROM=

# Optional: comma-separated admin numbers to notify (E.164 or local 10-digit numbers)
# Either of these env names will be read by the server:
ADMIN_NOTIFY_NUMBERS=7007120750,9807235659
ADMIN_WHATSAPP_NUMBERS=7007120750,9807235659

# Server port (optional)
PORT=4000
